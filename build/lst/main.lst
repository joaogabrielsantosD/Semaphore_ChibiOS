 165:main.c        **** void InitBuffer()
 166:main.c        **** {
 167:main.c        ****   chMtxObjectInit(&qmtx);
 168:main.c        ****   chCondObjectInit(&qempty);
 169:main.c        ****   chCondObjectInit(&qfull);
 170:main.c        ****  
 171:main.c        ****   rdp = wrp = &queue[0];
 172:main.c        ****   qsize = 0;
 173:main.c        **** }
 174:main.c        **** 
 175:main.c        **** void PushBUffer(msg_t msg)
 176:main.c        **** {
 177:main.c        ****   /* Entering monitor.*/
 178:main.c        ****   chMtxLock(&qmtx);
 179:main.c        ****  
 180:main.c        ****   /* Waiting for space in the queue.*/
 181:main.c        ****   while (IsBufferFull())
 182:main.c        ****     chCondWait(&qfull);
 183:main.c        ****  
 184:main.c        ****   /* Writing the message in the queue.*/  
 185:main.c        ****   *wrp = msg;
 186:main.c        ****   if (++wrp >= &queue[QUEUE_SIZE])
 187:main.c        ****     wrp = &queue[0];
 188:main.c        ****   qsize++;
 189:main.c        **** 
 190:main.c        ****   /* Signaling that there is at least a message.*/
 191:main.c        ****   chCondSignal(&qempty);
 192:main.c        **** 
 193:main.c        ****   /* Leaving monitor.*/
 194:main.c        ****   chMtxUnlock(&qmtx);
 195:main.c        **** }
 196:main.c        **** 
 197:main.c        **** uint8_t PopBUffer()
 198:main.c        **** {
 199:main.c        ****   msg_t msg;
 200:main.c        ****  
 201:main.c        ****   /* Entering monitor.*/
 202:main.c        ****   chMtxLock(&qmtx);
 203:main.c        ****  
 204:main.c        ****   /* Waiting for messages in the queue.*/
 205:main.c        ****   while (IsBUfferEmpty())
 206:main.c        ****     chCondWait(&qempty);
 207:main.c        ****  
 208:main.c        ****   /* Reading the message from the queue.*/  
 209:main.c        ****   msg = *rdp;
 210:main.c        ****   if (++rdp >= &queue[QUEUE_SIZE])
 211:main.c        ****     rdp = &queue[0];
 212:main.c        ****   qsize--;
 213:main.c        ****  
 214:main.c        ****   /* Signaling that there is at least one free slot.*/
 215:main.c        ****   chCondSignal(&qfull);
 216:main.c        ****  
 217:main.c        ****   /* Leaving monitor.*/
 218:main.c        ****   chMtxUnlock(&qmtx);
 219:main.c        ****  
 220:main.c        ****   return (uint8_t)msg;
 221:main.c        **** }
 222:main.c        **** 
 223:main.c        **** int IsBUfferEmpty()
 224:main.c        **** {
 225:main.c        ****   return qsize == 0;
 226:main.c        **** }
 227:main.c        **** 
 228:main.c        **** int IsBufferFull()
 229:main.c        **** {
 230:main.c        ****   return qsize >= QUEUE_SIZE;
 231:main.c        **** }

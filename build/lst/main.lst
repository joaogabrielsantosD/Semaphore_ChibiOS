 453:main.c        **** static void Avenida_Secundaria_Sinal_Amarelo(void *arg)
 454:main.c        **** {
 455:main.c        ****   chSysLockFromISR();
 456:main.c        ****   
 457:main.c        ****   counter++;
 458:main.c        **** 
 459:main.c        ****   if (counter >= 2)
 460:main.c        ****   {
 461:main.c        ****     counter = 0;
 462:main.c        ****     palClearPad(IOPORT4, LED_VERDE_SECUNDARIA);
 463:main.c        ****     palClearPad(IOPORT4, LED_AMARELO_SECUNDARIA);
 464:main.c        ****     palSetPad(IOPORT4, LED_VERMELHO_SECUNDARIA);
 465:main.c        **** 
 466:main.c        ****     if (amb_pri == 1)
 467:main.c        ****     {
 468:main.c        ****       STATE = PRINCIPAL;
 469:main.c        ****       LED_PRINCIPAL = VERDE;
 470:main.c        ****     }
 471:main.c        **** 
 472:main.c        ****     else if (((ped_sec >> 2) & ~0xFE) == 1)
 473:main.c        ****     {
 474:main.c        ****       ped_sec = 0;
 475:main.c        ****       STATE = PRINCIPAL;
 476:main.c        ****       LED_PRINCIPAL = VERDE;
 477:main.c        ****     }
 478:main.c        ****     
 479:main.c        ****     else if (EVENT == PEDESTRE)
 480:main.c        ****     {
 481:main.c        ****       ped_sec |= (1 << 1);
 482:main.c        ****       STATE = _PEDESTRE;
 483:main.c        ****       LED_PEDESTRE = VERDE;
 484:main.c        ****     }
 485:main.c        **** 
 486:main.c        ****     else
 487:main.c        ****     {
 488:main.c        ****       STATE = PRINCIPAL;
 489:main.c        ****       LED_PRINCIPAL = VERDE;
 490:main.c        ****     }
 491:main.c        **** 
 492:main.c        ****     EVENT = 0;
 493:main.c        ****     chVTReset((virtual_timer_t*)arg);
 494:main.c        ****   }

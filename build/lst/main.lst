 193:main.c        **** void InitBuffer()
 194:main.c        **** {
 195:main.c        ****   chMtxObjectInit(&qmtx);
 196:main.c        ****   chCondObjectInit(&qempty);
 197:main.c        ****   chCondObjectInit(&qfull);
 198:main.c        ****  
 199:main.c        ****   rdp = wrp = &queue[0];
 200:main.c        ****   qsize = 0;
 201:main.c        **** }
 202:main.c        **** 
 203:main.c        **** void PushBUffer(msg_t msg)
 204:main.c        **** {
 205:main.c        ****   /* Entering monitor.*/
 206:main.c        ****   chMtxLock(&qmtx);
 207:main.c        ****  
 208:main.c        ****   /* Waiting for space in the queue.*/
 209:main.c        ****   while (IsBufferFull())
 210:main.c        ****     chCondWait(&qfull);
 211:main.c        ****  
 212:main.c        ****   /* Writing the message in the queue.*/  
 213:main.c        ****   *wrp = msg;
 214:main.c        ****   if (++wrp >= &queue[QUEUE_SIZE])
 215:main.c        ****     wrp = &queue[0];
 216:main.c        ****   qsize++;
 217:main.c        **** 
 218:main.c        ****   /* Signaling that there is at least a message.*/
 219:main.c        ****   chCondSignal(&qempty);
 220:main.c        **** 
 221:main.c        ****   /* Leaving monitor.*/
 222:main.c        ****   chMtxUnlock(&qmtx);
 223:main.c        **** }
 224:main.c        **** 
 225:main.c        **** uint8_t PopBUffer()
 226:main.c        **** {
 227:main.c        ****   msg_t msg;
 228:main.c        ****  
 229:main.c        ****   /* Entering monitor.*/
 230:main.c        ****   chMtxLock(&qmtx);
 231:main.c        ****  
 232:main.c        ****   /* Waiting for messages in the queue.*/
 233:main.c        ****   while (IsBUfferEmpty())
 234:main.c        ****     chCondWait(&qempty);
 235:main.c        ****  
 236:main.c        ****   /* Reading the message from the queue.*/  
 237:main.c        ****   msg = *rdp;
 238:main.c        ****   if (++rdp >= &queue[QUEUE_SIZE])
 239:main.c        ****     rdp = &queue[0];
 240:main.c        ****   qsize--;
 241:main.c        ****  
 242:main.c        ****   /* Signaling that there is at least one free slot.*/
 243:main.c        ****   chCondSignal(&qfull);
 244:main.c        ****  
 245:main.c        ****   /* Leaving monitor.*/
 246:main.c        ****   chMtxUnlock(&qmtx);
 247:main.c        ****  
 248:main.c        ****   return (uint8_t)msg;
 249:main.c        **** }

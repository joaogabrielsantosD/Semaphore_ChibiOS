 242:main.c        ****       palClearPad(IOPORT4, LED_VERMELHO_PRINCIPAL);
 243:main.c        ****       chVTSet(&vt, TIME_MS2I(1000), AvenidaPrincipalSinalVerde, (void*)&vt);
 244:main.c        ****       break;
 245:main.c        ****     }
 246:main.c        **** 
 247:main.c        ****     case AMARELO:
 248:main.c        ****     {
 249:main.c        ****       palClearPad(IOPORT2, LED_VERDE_PRINCIPAL);
 250:main.c        ****       palClearPad(IOPORT4, LED_VERMELHO_PRINCIPAL);
 251:main.c        ****       palSetPad(IOPORT4, LED_AMARELO_PRINCIPAL);
 252:main.c        ****       chVTSet(&vt, TIME_MS2I(1000), AvenidaPrincipalSinalAmarelo, (void*)&vt);
 253:main.c        ****       break;
 254:main.c        ****     }
 255:main.c        ****   }  
 256:main.c        **** }

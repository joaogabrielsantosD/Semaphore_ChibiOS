 272:main.c        **** void InitBuffer()
 273:main.c        **** {
 274:main.c        ****   chMtxObjectInit(&qmtx);
 275:main.c        ****   chCondObjectInit(&qempty);
 276:main.c        ****   chCondObjectInit(&qfull);
 277:main.c        ****  
 278:main.c        ****   rdp = wrp = &queue[0];
 279:main.c        ****   qsize = 0;
 280:main.c        **** }
 281:main.c        **** 
 282:main.c        **** void PushBUffer(msg_t msg)
 283:main.c        **** {
 284:main.c        ****   /* Entering monitor.*/
 285:main.c        ****   chMtxLock(&qmtx);
 286:main.c        ****  
 287:main.c        ****   /* Waiting for space in the queue.*/
 288:main.c        ****   while (IsBufferFull())
 289:main.c        ****     chCondWait(&qfull);
 290:main.c        ****  
 291:main.c        ****   /* Writing the message in the queue.*/  
 292:main.c        ****   *wrp = msg;
 293:main.c        ****   if (++wrp >= &queue[QUEUE_SIZE])
 294:main.c        ****     wrp = &queue[0];
 295:main.c        ****   qsize++;
 296:main.c        **** 
 297:main.c        ****   /* Signaling that there is at least a message.*/
 298:main.c        ****   chCondSignal(&qempty);
 299:main.c        **** 
 300:main.c        ****   /* Leaving monitor.*/
 301:main.c        ****   chMtxUnlock(&qmtx);
 302:main.c        **** }
 303:main.c        **** 
 304:main.c        **** uint8_t PopBUffer()
 305:main.c        **** {
 306:main.c        ****   msg_t msg;
 307:main.c        ****  
 308:main.c        ****   /* Entering monitor.*/
 309:main.c        ****   chMtxLock(&qmtx);
 310:main.c        ****  
 311:main.c        ****   /* Waiting for messages in the queue.*/
 312:main.c        ****   while (IsBUfferEmpty())
 313:main.c        ****     chCondWait(&qempty);
 314:main.c        ****  
 315:main.c        ****   /* Reading the message from the queue.*/  
 316:main.c        ****   msg = *rdp;
 317:main.c        ****   if (++rdp >= &queue[QUEUE_SIZE])
 318:main.c        ****     rdp = &queue[0];
 319:main.c        ****   qsize--;
 320:main.c        ****  
 321:main.c        ****   /* Signaling that there is at least one free slot.*/
 322:main.c        ****   chCondSignal(&qfull);
 323:main.c        ****  
 324:main.c        ****   /* Leaving monitor.*/
 325:main.c        ****   chMtxUnlock(&qmtx);
 326:main.c        ****  
 327:main.c        ****   return (uint8_t)msg;
 328:main.c        **** }

 267:main.c        ****   if (counter >= 40 && (EVENT == PEDESTRE || EVENT == CARRO_SECUNDARIA))
 268:main.c        ****   {
 269:main.c        ****     counter = 0;
 270:main.c        ****     palClearPad(IOPORT2, LED_VERDE_PRINCIPAL);
 271:main.c        ****     STATE = PRINCIPAL;
 272:main.c        ****     LED_PRINCIPAL = AMARELO;
 273:main.c        ****     chVTReset((virtual_timer_t*)arg);
 274:main.c        ****   }
 275:main.c        **** 
 276:main.c        ****   if (counter >= 10 && EVENT == AMBULANCIA_SECUNDARIA)
 277:main.c        ****   {
 278:main.c        ****     counter = 0;
 279:main.c        ****     palClearPad(IOPORT2, LED_VERDE_PRINCIPAL);
 280:main.c        ****     STATE = PRINCIPAL;
 281:main.c        ****     LED_PRINCIPAL = AMARELO;
 282:main.c        ****     chVTReset((virtual_timer_t*)arg);
 283:main.c        ****   }
 284:main.c        **** 
 285:main.c        ****   chVTSetI((virtual_timer_t*)arg, TIME_MS2I(1000), AvenidaPrincipalSinalVerde, arg);
 286:main.c        ****   chSysUnlockFromISR();
 287:main.c        **** }

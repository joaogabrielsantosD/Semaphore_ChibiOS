 423:main.c        **** static void Avenida_Secundaria_Sinal_Verde(void *arg)
 424:main.c        **** {
 425:main.c        ****   chSysLockFromISR();
 426:main.c        **** 
 427:main.c        ****   if (amb_sec == 0)
 428:main.c        ****   {
 429:main.c        ****     counter++;
 430:main.c        **** 
 431:main.c        ****     if (counter >= 6)
 432:main.c        ****     {
 433:main.c        ****       counter = 0;
 434:main.c        ****       STATE = SECUNDARIA;
 435:main.c        ****       LED_SECUNDARIA = AMARELO;
 436:main.c        ****       chVTReset((virtual_timer_t*)arg);
 437:main.c        ****     }
 438:main.c        **** 
 439:main.c        ****     if (counter >= 5 && amb_pri == 1)
 440:main.c        ****     {
 441:main.c        ****       counter = 0;
 442:main.c        ****       palClearPad(IOPORT4, LED_VERDE_SECUNDARIA);
 443:main.c        ****       STATE = SECUNDARIA;
 444:main.c        ****       LED_SECUNDARIA = AMARELO;
 445:main.c        ****       chVTReset((virtual_timer_t*)arg);
 446:main.c        ****     }
 447:main.c        ****   }
 448:main.c        **** 
 449:main.c        ****   chVTSetI((virtual_timer_t*)arg, TIME_MS2I(1000), Avenida_Secundaria_Sinal_Verde, arg);
 450:main.c        ****   chSysUnlockFromISR();  
 451:main.c        **** }

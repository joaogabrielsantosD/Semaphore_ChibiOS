 216:main.c        **** void InitBuffer()
 217:main.c        **** {
 218:main.c        ****   chMtxObjectInit(&qmtx);
 219:main.c        ****   chCondObjectInit(&qempty);
 220:main.c        ****   chCondObjectInit(&qfull);
 221:main.c        ****  
 222:main.c        ****   rdp = wrp = &queue[0];
 223:main.c        ****   qsize = 0;
 224:main.c        **** }
 225:main.c        **** 
 226:main.c        **** void PushBUffer(msg_t msg)
 227:main.c        **** {
 228:main.c        ****   /* Entering monitor.*/
 229:main.c        ****   chMtxLock(&qmtx);
 230:main.c        ****  
 231:main.c        ****   /* Waiting for space in the queue.*/
 232:main.c        ****   while (IsBufferFull())
 233:main.c        ****     chCondWait(&qfull);
 234:main.c        ****  
 235:main.c        ****   /* Writing the message in the queue.*/  
 236:main.c        ****   *wrp = msg;
 237:main.c        ****   if (++wrp >= &queue[QUEUE_SIZE])
 238:main.c        ****     wrp = &queue[0];
 239:main.c        ****   qsize++;
 240:main.c        **** 
 241:main.c        ****   /* Signaling that there is at least a message.*/
 242:main.c        ****   chCondSignal(&qempty);
 243:main.c        **** 
 244:main.c        ****   /* Leaving monitor.*/
 245:main.c        ****   chMtxUnlock(&qmtx);
 246:main.c        **** }
 247:main.c        **** 
 248:main.c        **** uint8_t PopBUffer()
 249:main.c        **** {
 250:main.c        ****   msg_t msg;
 251:main.c        ****  
 252:main.c        ****   /* Entering monitor.*/
 253:main.c        ****   chMtxLock(&qmtx);
 254:main.c        ****  
 255:main.c        ****   /* Waiting for messages in the queue.*/
 256:main.c        ****   while (IsBUfferEmpty())
 257:main.c        ****     chCondWait(&qempty);
 258:main.c        ****  
 259:main.c        ****   /* Reading the message from the queue.*/  
 260:main.c        ****   msg = *rdp;
 261:main.c        ****   if (++rdp >= &queue[QUEUE_SIZE])
 262:main.c        ****     rdp = &queue[0];
 263:main.c        ****   qsize--;
 264:main.c        ****  
 265:main.c        ****   /* Signaling that there is at least one free slot.*/
 266:main.c        ****   chCondSignal(&qfull);
 267:main.c        ****  
 268:main.c        ****   /* Leaving monitor.*/
 269:main.c        ****   chMtxUnlock(&qmtx);
 270:main.c        ****  
 271:main.c        ****   return (uint8_t)msg;
 272:main.c        **** }
 273:main.c        **** 
 274:main.c        **** int IsBUfferEmpty()
 275:main.c        **** {
 276:main.c        ****   return qsize == 0;
 277:main.c        **** }
 278:main.c        **** 
 279:main.c        **** int IsBufferFull()
 280:main.c        **** {
 281:main.c        ****   return qsize >= QUEUE_SIZE;
 282:main.c        **** }
